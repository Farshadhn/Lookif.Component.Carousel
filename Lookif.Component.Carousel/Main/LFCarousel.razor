@using Lookif.Component.Carousel.Header;
@using Lookif.Component.Carousel.Items;
@inherits LFCarouselBase 


@if (CarouselType == CarouselType.Header)
{
    <HeaderCarousel Data=Data NotFoundImage="@NotFoundImage"></HeaderCarousel>
} else if (CarouselType == CarouselType.Item)
{
    <ItemCarousel Data=Data Size=Size NotFoundImage="@NotFoundImage"></ItemCarousel>

}
